{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "askedio/laravel-soft-cascade": "^6.0",
        "creativeorange/gravatar": "~1.0",
        "doctrine/dbal": "^2.9",
        "fideloper/proxy": "^4.0",
        "glorand/laravel-model-settings": "^3.5",
        "illuminatech/validation-composite": "^1.0",
        "laravel/framework": "^6.2",
        "laravel/horizon": "^3.4",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/socialite": "^4.2",
        "laravel/telescope": "^2.1",
        "laravel/tinker": "^1.0",
        "laravelcollective/annotations": "6.0.*",
        "league/flysystem-aws-s3-v3": "~1.0",
        "league/flysystem-cached-adapter": "~1.0",
        "predis/predis": "^1.1",
        "santutu/laravel-dotenv": "^2.0",
        "spatie/laravel-permission": "^3.2",
        "thepinecode/blade-filters": "^0.4.0",
        "tightenco/quicksand": "^2.2",
        "torann/geoip": "^1.0",
        "webmozart/path-util": "^2.3",
        "cartalyst/tags": "^9.0",
        "arcanedev/log-viewer": "^5.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "caouecs/laravel-lang": "~4.0",
        "facade/ignition": "^1.4",
        "facade/ignition-tinker-tab": "^1.0",
        "fzaninotto/faker": "^1.4",
        "laravel/ui": "^1.1",
        "mockery/mockery": "^1.0",
        "mpociot/laravel-apidoc-generator": "^3.17",
        "mpociot/laravel-test-factory-helper": "^1.2",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "roave/security-advisories": "dev-master"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:models -W -n",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
